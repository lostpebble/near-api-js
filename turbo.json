{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.ts", "test/**/*.js"],
      "outputs": ["dist/**", "lib/**"]
    },
    "test": {
      "dependsOn": [],
      "inputs": ["src/**/*.ts", "test/**/*.js"]
    },
    "lint": {
      "inputs": ["src/**/*.ts", "test/**/*.ts"],
      "outputs": []
    },
    "lint:fix": {
      "inputs": ["src/**/*.ts", "test/**/*.ts"],
      "outputs": []
    },
    "clean": {
      "outputs": [],
      "cache": false
    }
  }
}
